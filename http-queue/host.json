{
  "version": "2.0",

  "customHandler": {
    "description": {
      "defaultExecutablePath": "/Users/antchu/go/bin/dlv",
      "workingDirectory": "",
      "arguments": [
        "exec",
        "./server",
        "-l 127.0.0.1:2345",
        "--api-version 2",
        "--headless"
      ]
    },
    "enableForwardingHttpRequest": true
  },

  "logging": {
    "logLevel": {
      "default": "Warning",
      "Function.hello.User": "Information",
      "Function.order.User": "Information",
      "Function.queueTrigger.User": "Information"
    }
  },
  
  "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[2.*, 3.0.0)"
  }
}